{
  "applicationTimeout": "60,0,0,0",
  "clientCookies": "false",
  "clientManagement": "false",
  "clientTimeout": "0,0,90,0",
  "datasourcePreserveSingleQuotes": "yes",
  "datasources": {
    "spendingtracker": {
      "allowAlter": "true",
      "allowCreate": "false",
      "allowDelete": "false",
      "allowDrop": "false",
      "allowGrant": "false",
      "allowInsert": "true",
      "allowRevoke": "false",
      "allowSelect": "true",
      "allowUpdate": "true",
      "blob": "true",
      "bundleName": "${DB_BUNDLENAME}",
      "bundleVersion": "${DB_BUNDLEVERSION}",
      "class": "${DB_CLASS}",
      "clob": "false",
      "connectionLimit": "-1",
      "connectionTimeout": "5",
      "custom": "",
      "database": "${DB_DATABASE}",
      "dbdriver": "${DB_DRIVER}",
      "dsn": "jdbc:${DB_DRIVER}://${DB_HOST}:${DB_PORT}/${DB_DATABASE}",
      "host": "${DB_HOST}",
      "password": "${DB_PASSWORD}",
      "port": "${DB_PORT}",
      "storage": "false",
      "timezone": "UTC",
      "username": "${DB_USERNAME}"
    }
  },
  "domainCookies": "true",
  "dotNotationUpperCase": "false",
  "inspectTemplate": "${INSPECTTEMPLATE}",
  "mailConnectionTimeout": "30",
  "mailDefaultEncoding": "UTF-8",
  "mailServers": [
    {
      "smtp": "${EMAIL_SERVER}",
      "username": "${EMAIL_USERNAME}",
      "password": "${EMAIL_PASSWORD}",
      "port": "${EMAIL_PORT}",
      "tls": "${EMAIL_TLS}",
      "ssl": "${EMAIL_SSL}",
      "reuseConnection": "true",
      "idleTimeout": "9000",
      "lifeTimeout": "59000"
    }
  ],
  "mailSpoolEnable": "${EMAIL_SPOOL}",
  "mailSpoolInterval": "5",
  "requestQueueEnable": "true",
  "requestQueueTimeout": "600000",
  "requestTimeoutEnabled": "true",
  "requestTimeout": "0,0,0,${REQUESTTIMEOUT}",
  "resourceCharset": "UTF-8",
  "robustExceptionEnabled": "true",
  "scopeCascading": "strict",
  "sessionManagement": "false",
  "sessionTimeout": "0,0,30,0",
  "sessionType": "j2ee",
  "templateCharset": "UTF-8",
  "thisTimeZone": "UTC",
  "webCharset": "UTF-8",
  "whitespaceManagement": "white-space-pref"
}
